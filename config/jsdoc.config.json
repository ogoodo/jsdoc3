{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "destination": "../build-docs"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "markdown": {
    "tags": ["这里可以扩展使用markdown的标签", "foo"],
    "excludeTags": ["这里可以排除不用的标签"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "path": "ink-docstrap",
    "theme": "cerulean",
    "navType": "vertical",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  }
}
